{
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.markdownlint": true
  },
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "editor.formatOnPaste": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "cSpell.words": [
    "clsx",
    "darktheme",
    "ECENT",
    "Extralight",
    "fontsource",
    "formspree",
    "grotesk",
    "hanken",
    "hightlight",
    "iefix",
    "immer",
    "lighttheme",
    "macbook",
    "Mori",
    "mzbqrjgj",
    "noarchive",
    "nums",
    "opentype",
    "stylesheet",
    "techstack",
    "Transfonter",
    "truetype",
    "Webflow",
    "whyme"
  ],
  "liveServer.settings.port": 5501,
  "cSpell.ignoreWords": ["languagedetector", "teckstack"]
}
